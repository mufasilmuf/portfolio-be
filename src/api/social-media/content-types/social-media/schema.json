{
  "kind": "collectionType",
  "collectionName": "social_medias",
  "info": {
    "singularName": "social-media",
    "pluralName": "social-medias",
    "displayName": "socialMedia"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "regex": "/((([A-Za-z]{3,9}:(?:\\/\\/)?)(?:[-;:&=\\+\\$,\\w]+@)?[A-Za-z0-9.-]+|(?:www.|[-;:&=\\+\\$,\\w]+@)[A-Za-z0-9.-]+)((?:\\/[\\+~%\\/.\\w-_]*)?\\??(?:[-\\+=&;%@.\\w_]*)#?(?:[\\w]*))?)/",
      "required": true
    },
    "logo": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "orderId": {
      "type": "integer",
      "required": true
    }
  }
}
