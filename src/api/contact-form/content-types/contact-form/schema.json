{
  "kind": "collectionType",
  "collectionName": "contact_forms",
  "info": {
    "singularName": "contact-form",
    "pluralName": "contact-forms",
    "displayName": "contactForm",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "websiteUrl": {
      "type": "string",
      "regex": "/((([A-Za-z]{3,9}:(?:\\/\\/)?)(?:[-;:&=\\+\\$,\\w]+@)?[A-Za-z0-9.-]+|(?:www.|[-;:&=\\+\\$,\\w]+@)[A-Za-z0-9.-]+)((?:\\/[\\+~%\\/.\\w-_]*)?\\??(?:[-\\+=&;%@.\\w_]*)#?(?:[\\w]*))?)/",
      "required": false
    },
    "message": {
      "type": "text",
      "required": true
    },
    "createAt": {
      "type": "datetime",
      "required": true
    }
  }
}
