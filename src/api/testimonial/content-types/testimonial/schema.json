{
  "kind": "collectionType",
  "collectionName": "testimonials",
  "info": {
    "singularName": "testimonial",
    "pluralName": "testimonials",
    "displayName": "Testimonials"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "feedback": {
      "type": "text",
      "required": true
    },
    "linkedInUrl": {
      "type": "text",
      "regex": "/((([A-Za-z]{3,9}:(?:\\/\\/)?)(?:[-;:&=\\+\\$,\\w]+@)?[A-Za-z0-9.-]+|(?:www.|[-;:&=\\+\\$,\\w]+@)[A-Za-z0-9.-]+)((?:\\/[\\+~%\\/.\\w-_]*)?\\??(?:[-\\+=&;%@.\\w_]*)#?(?:[\\w]*))?)/"
    },
    "profileImg": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "desgination": {
      "type": "string",
      "required": true
    },
    "company": {
      "type": "string"
    }
  }
}
